int middle(int A, int B, int C){
        if (A>B){
            if(B>C){
                return B;
            }
            else{
                return C;
            }
        }
        else{
            if(A>C){
                return A;
            }
            else if(B>C){
                return C;
            }
            else if(C>B){
                return B;
            }
        }
    }
